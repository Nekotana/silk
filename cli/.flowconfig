[ignore]
.*/node_modules/babel.*
.*/node_modules/mocha.*
.*/node_modules/json5/test/.*
.*/node_modules/assert/.*

[include]

[libs]

[options]
strip_root=true
module.system=haste
esproposal.class_instance_fields=ignore
esproposal.class_static_fields=ignore
unsafe.enable_getters_and_setters=true
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe

[version]
0.28.0
